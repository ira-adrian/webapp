{% extends 'ExpedienteBundle:Default:layout.html.twig' %}
{% block title %}  {% endblock %}


{% block main %}  
    <h1>Tipodocumento</h1>
<div class="nav nav-pills hidden-print"> 
      <li role="presentation" class="disabled"><a href="#">Menu</a></li>
        <li role="presentation">
             <a href="{{ path('tipodocumento_index') }}">Volver a la Lista</a>
        </li>
         <li role="presentation">
            <a href="{{ path('tipodocumento_edit', { 'id': tipoDocumento.id }) }}">Modificar</a>
        </li>
        <li role="presentation">
            {{ form_start(delete_form) }}
                <input type="submit" value="Eliminar">
            {{ form_end(delete_form) }}
        </li>
</div>
    <table>
        <tbody>
            <tr>
            <th>Id</th>
                <td>{{ tipoDocumento.id }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ tipoDocumento.slug }}</td>
            </tr>
            <tr>
                <th>Nombredocumento</th>
                <td>{{ tipoDocumento.nombreDocumento }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ tipoDocumento.numero }}</td>
            </tr>
            <tr>
                <th>Anio</th>
                <td>{{ tipoDocumento.anio }}</td>
            </tr>
        </tbody>
    </table>
{{ tipoDocumento.texto | raw }}

{% endblock %}
