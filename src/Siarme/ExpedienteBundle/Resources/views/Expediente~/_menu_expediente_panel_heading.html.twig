   {% if  (expediente.datoAt is not empty ) %}
        {% if   date(expediente.datoAt.fechaTurno )  >=  date('now')  or expediente.datoAt.fechaTurno  | date('d-m-Y')  ==  'now' | date('d-m-Y')%}
            <span> 
             <a class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">{{ include('ExpedienteBundle:Expediente:_turno_etiqueta.html.twig') }} 
            </a> 
            <div  class="dropdown-menu" style="width:20em;">                     
                    <ul class="list-group text-center">
                        <li class="list-group-item">
                           Turno: <strong>{{expediente.datoAt.fechaTurno | localizeddate('full', 'none', 'es')}} </strong>
                           {% if expediente.datoAt.estado == "Sin Datos"%}
                              <strong> Sin Fecha</strong>
                            {% else %}
                                 - Hora: <strong> {{expediente.datoAt.hora | date('H:i:s')}}</strong>
                            {% endif %} 
                        </li>
                    </ul>                   
            </div>
          </span>             
         {% endif %}
{% endif %} 
 {% set licenciaActivaArray = [] %} 
  {% set cantidadDias = 0 %}
  {% set cantidadDiasAmpliacion = 0 %}
 {% set documentoMedicoActivoArray = [] %} 
 {% set documentosArray = [] %} 
            {% for tramite in expediente.tramite | sort | reverse  %} 
               {% for documento in tramite.documento %}    
      
                  {% set documentosArray = documentosArray|merge([documento]) %}        
               
              {% endfor %} 
           {% endfor %} 
{% if documentosArray | length > 0 %}
    <span data-toggle="tooltip" data-placement="top" title="Documentos">
    <a class="dropdown-toggle previous hidden-print" data-toggle="dropdown" role="button" aria-expanded="false">{% if documentosArray | length > 0 %}<span  class="glyphicon glyphicon-paperclip text-info" aria-hidden="false">{{ documentosArray | length }}</span>{% else %}<span  class="glyphicon glyphicon-paperclip text-muted" aria-hidden="false">{{ documentosArray | length }}</span>{% endif %}</a>
       <ul  class="dropdown-menu"  style="width:20em;">  <!-- dropdown-menu -->  
                {% for documento in documentosArray %}                                  
                       {% if documento.papelera  %}
                               {%  set detalle = "Documento en Papelera" %}
                       {% elseif documento.slug == "archivo" %}
                              {% set   detalle= documento.texto %}   

                         {% else %}
                          {%  set detalle= documento %}   
                            
                          {% if documento.slug == "licencia"  %} 
                                 {% set cantidadDias = cantidadDias + documento.licencia.dias %} 
                                
                                 {% if   ((documento.licencia.fechaDesde  | date('d-m-Y')  <=  'now' | date('d-m-Y')) and (documento.licencia.fechaHasta  | date('d-m-Y')  >=  'now' | date('d-m-Y')) ) %} 
                                          {% set licenciaActivaArray = licenciaActivaArray|merge([documento.licencia]) %}
                                 {% endif %}   
                         {% endif %}
                       {% if documento.slug == "ampliacion"  %}

                         {% set cantidadDiasAmpliacion = cantidadDiasAmpliacion  + documento.licencia.dias %} 

                       {% endif %}
                   {% if documento.slug == "acta-medica"  %}
                         {% set documentoMedicoActivoArray = documentoMedicoActivoArray|merge([documento.licencia]) %}
                      {% if   ((documento.licencia.fechaDesde  | date('d-m-Y')  <= 'now' | date('d-m-Y')) and (documento.licencia.fechaHasta  | date('d-m-Y')  >=  'now' | date('d-m-Y')) ) %} 
                      {% set documentoMedicoActivoArray = documentoMedicoActivoArray|merge([documento.licencia]) %}
                       {% endif %} 
                  {% endif %}

                     {% endif %}               
           <li class="list-group" style="margin:0px;" >   <!-- list-group -->       
                        <a href="{{ path('documento_show', { 'id': documento.id }) }}">                              
          {% if documento.papelera == true  %}
                <span style="font-size: 20px"              
                class='{{documento.tipodocumento.glyphicon ~ ' text-muted'}}'>
                </span>
                  {%  set color = " text-muted" %} 
          {% else %}

                <span style="font-size: 20px" class='{{documento.tipodocumento.glyphicon ~ ' text-primary'}}'></span>

                {%  set color = " text-primary" %} 
          {% endif %} 
                {% if ( app.user.id == documento.usuario.id) %}
                        <span data-toggle="tooltip" data-placement="top" title='{{ documento.usuario | title }}' class='glyphicon glyphicon-cog text-success' aria-hidden="true"></span>  
                        {% else %}       
                         <span data-toggle="tooltip" data-placement="top" title='{{ documento.usuario | title }}' class='glyphicon glyphicon-cog  text-muted' aria-hidden="true"></span> 
              {% endif %}   {{ documento }}
                            </a> 
                </li> <!-- list-group --> 
                {% endfor %}
                 </ul> <!-- dropdown-menu -->  
        </span> &nbsp; 
{% endif %}   
{% if cantidadDias > 0 %}
   <span data-toggle="tooltip" data-placement="top" title= "Días">
              {% if expediente.letra == "LIC"  %} 
              <a class="dropdown-toggle text-info" data-toggle="dropdown" role="button" aria-expanded="false"><span  class="glyphicon glyphicon-stats text-info" aria-hidden="true"></span>{{ cantidadDias }}</a> 
              <div  class="dropdown-menu" style="width:20em;">                     
                    <ul class="list-group text-center text-upper">
                        <li class="list-group-item">
                            Días usufructuados:<strong> {{ cantidadDias }} </strong>
                        </li>
                         <li class="list-group-item">
                            Días por Articulo: <strong> {{ expediente.articulo.dias  | default(0)}} </strong>
                        </li>
                         <li class="list-group-item">
                            Ampliaciones: <strong> {{ cantidadDiasAmpliacion  | default(0)}} </strong>
                        </li>
                         <li class="list-group-item">
                           Días restantes: <strong> {{ expediente.articulo.dias | default(0) + cantidadDiasAmpliacion | default(0)  - cantidadDias }} </strong> 
                        </li>
                    </ul>                   
              </div> 
              {% endif %}
</span>&nbsp;
{% endif %}
{% if licenciaActivaArray | length > 0 %}
<span  data-toggle="tooltip" data-placement="top" title= "Licencia EN CURSO">
               <a class="dropdown-toggle text-info" data-toggle="dropdown" role="button" aria-expanded="false"><span class="glyphicon glyphicon-tags text-success" aria-hidden="true"></span>{{ licenciaActivaArray | length }}</a> 
              <div  class="dropdown-menu" style="width:20em; margin:2px;">                     
                    <ul class="list-group text-center text-upper">
                       {% for licencia in licenciaActivaArray %}
                        <li class="list-group-item">
                           <a href="{{ path('documento_show', { 'id': licencia.id }) }}">
                           <span  class='{{licencia.documento.tipodocumento.glyphicon ~ ' text-success'}}'></span>  Licencia: {{ licencia }}</a>
                        </li>
                         {% endfor %}
                    </ul>                   
              </div> 

              
</span>&nbsp;
{% endif %}
{% if documentoMedicoActivoArray | length > 0 %}
<span  data-toggle="tooltip" data-placement="top" title= "Benefico EN CURSO">
               <a class="dropdown-toggle text-info" data-toggle="dropdown" role="button" aria-expanded="false"><span class='glyphicon glyphicon-plus-sign text-success' aria-hidden="true"></span>{{ documentoMedicoActivoArray | length }}</a> 
              <div  class="dropdown-menu" style="width:20em; margin:2px;">                     
                    <ul class="list-group text-center text-upper">
                        {% for beneficio in documentoMedicoActivoArray if documentoMedicoActivoArray | length > 0 %}
                        <li class="list-group-item">
                           <a href="{{ path('documento_show', { 'id': beneficio.id }) }}">
                           <span  class='{{beneficio.documento.tipodocumento.glyphicon ~ ' text-success'}}'></span>  Beneficio: {{ beneficio }}</a>
                          
                        </li>
                          {% endfor %}
                    </ul>                   
              </div> 
</span>&nbsp;
{% endif %}
   {% if  (expediente.datoAt is not empty ) %}   
                        {# en la Base se guarda "Aceptado" pero en pantalla se muestra "otorgado" #}
      {% if expediente.datoAt.estado == "Aceptado" or expediente.datoAt.estado == "Otorgado"%}
               &nbsp;<span  data-toggle="tooltip" data-placement="top" title='Beneficio Otorgado'  ><span class="glyphicon glyphicon-thumbs-up text-info"  aria-hidden="true"></span></span>

      {% elseif expediente.datoAt.estado == "Denegado"   %}
               &nbsp;<span data-toggle="tooltip" data-placement="top" title='Beneficio Denegado' ><span class="glyphicon glyphicon-thumbs-down" aria-hidden="true"></span></span>

      {% endif %}  

      {% if expediente.datoAt.altaLaboral  %}
               &nbsp;<span data-toggle="tooltip" data-placement="top" title='Con Alta Laboral' ><span class="glyphicon glyphicon-heart-empty text-info" aria-hidden="true"></span></span>

      {% endif %}  

      {% if expediente.datoAt.altaMedica   %}
               &nbsp;<span data-toggle="tooltip" data-placement="top" title='Con Alta Medica' ><span class="glyphicon glyphicon-heart text-info" aria-hidden="true"></span></span>

      {% endif %}  
  {% endif %} 







